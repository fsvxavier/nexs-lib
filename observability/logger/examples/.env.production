# Configuração de Logging - Produção
LOG_LEVEL=info
LOG_FORMAT=json
LOG_ADD_SOURCE=false
LOG_ADD_STACKTRACE=true
LOG_TIME_FORMAT=RFC3339
LOG_PROVIDER=zap

# Informações do Serviço
SERVICE_NAME=isis-api
SERVICE_VERSION=2.1.0
ENVIRONMENT=production

# Sampling para alto volume
LOG_SAMPLING_INITIAL=1000
LOG_SAMPLING_THEREAFTER=100
LOG_SAMPLING_TICK=1s
